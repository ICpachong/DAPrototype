<?xml version="1.0"?>

<SETTINGS>
	<GEN>
		<!Value of 0 disables>
		<enBuzzer>1</enBuzzer>
		<debugScreen>1</debugScreen>
		<debugTerminal>1</debugTerminal>
	</GEN>
	<CAM>
		<recordOrgImage>0</recordOrgImage>
		<filePath>./</filePath>
		<orgFileName>intest.avi</orgFileName>
		<modFileName>outtest.avi</modFileName>
		<pixWidth>640</pixWidth>
		<pixHeight>480</pixHeight>
		<showSpeed>1</showSpeed>
		<showLoc>1</showLoc>
		<shadeLanes>1</shadeLanes>
		<recFPS>15</recFPS>
		<filesToKeep>20</filesToKeep>
		<minPerFile>1</minPerFile>
	</CAM>
	<DISP>
		<enabled>0</enabled>
		<pixWidth>800</pixWidth>
		<pixHeight>480</pixHeight>
		<updateFPS>10</updateFPS>
	</DISP>
	<COMM>
		<pollRateGPS>10</pollRateGPS>
		<pollRateLIDAR>10</pollRateLIDAR>
		<pollRateGpio>10</pollRateGpio>
	</COMM>
	<GPS>
		<samplesToAverage>3</samplesToAverage>
	</GPS>
	<LDW>
		<enabled>1</enabled>
		<enableSpeed>30</enableSpeed>
		<samplesToKeep>10</samplesToKeep>
		<samplesToAverage>6</samplesToAverage>
		<!Warning limit as % of screen width>
		<perOffsetWarning>15</perOffsetWarning>
		<!Alarm limit as % of screen width>
		<perOffsetAlarm>20</perOffsetAlarm>
		<msUntilWarning>500</msUntilWarning>
		<msUntilAlarm>500</msUntilAlarm>
		<updateFPS>15</updateFPS>
	</LDW>
	<FCW>
		<enabled>0</enabled>
		<samplesToAverage>3</samplesToAverage>
		<distanceOffset>1.2</distanceOffset>
		<msFollowDistWarning>2500</msFollowDistWarning>
		<msFollowDistAlarm>2000</msFollowDistAlarm>
		<msCollisionWarning>300</msCollisionWarning>
		<msCollisionAlarm>300</msCollisionAlarm>
	</FCW>
	<GPIO>
		<enabled>1</enabled>
		<autoShutdown>1</autoShutdown>
	</GPIO>
</SETTINGS>